<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:my="clr-namespace:Snoop;assembly="
	xmlns:d="http://schemas.microsoft.com/expression/blend/2006" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
	x:Class="Snoop.AppChooser"
	Name="RootWindow"
	Icon="Snoop.ico"
	Title="Snoop"
	Height="26"
	DataContext="{Binding RelativeSource={RelativeSource Self}}"
	Background="{x:Null}" Foreground="{x:Null}" ResizeMode="NoResize" SizeToContent="Width" MouseLeftButtonDown="HandleMouseLeftButtonDown"
	SnapsToDevicePixels="False" AllowsTransparency="True"
	WindowStyle="None"
	Topmost="True"
>
	<Window.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Icons.xaml"/>
				<ResourceDictionary Source="Simple Styles.xaml"/>
			</ResourceDictionary.MergedDictionaries>
			<Style TargetType="{x:Type Button}" x:Key="ReloadButton">
				<Setter Property="FocusVisualStyle">
					<Setter.Value>
						<Style>
							<Setter Property="Control.Template">
								<Setter.Value>
									<ControlTemplate>
										<Rectangle Stroke="#FF000000" StrokeThickness="1" Margin="3,3,3,3" x:Name="Rectangle" SnapsToDevicePixels="True" StrokeDashArray="1 2"/>
									</ControlTemplate>
								</Setter.Value>
							</Setter>
						</Style>
					</Setter.Value>
				</Setter>
				<Setter Property="Background">
					<Setter.Value>
						<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
							<GradientStop Color="#FF69E861" Offset="0"/>
							<GradientStop Color="#FFE3E5F0" Offset="0.5"/>
							<GradientStop Color="#FF1CC211" Offset="1"/>
						</LinearGradientBrush>
					</Setter.Value>
				</Setter>
				<Setter Property="BorderBrush" Value="#FF003C74"/>
				<Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
				<Setter Property="HorizontalContentAlignment" Value="Center"/>
				<Setter Property="VerticalContentAlignment" Value="Center"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="{x:Type ButtonBase}">
							<Grid Width="Auto" Height="Auto">
								<Border x:Name="border" BorderBrush="{DynamicResource DarkBlue}" BorderThickness="1,1,1,1" CornerRadius="4,4,4,4">
									<Border.Background>
										<DynamicResource ResourceKey="MetalGrad"/>
									</Border.Background>
								</Border>
								<ContentPresenter Margin="2,2,2,2"/>
							</Grid>
							<ControlTemplate.Triggers>
								<Trigger Property="IsKeyboardFocused" Value="True"/>
								<Trigger Property="ToggleButton.IsChecked" Value="True"/>
								<Trigger Property="IsEnabled" Value="False">
									<Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
								</Trigger>
								<Trigger Property="IsMouseOver" Value="True">
									<Setter Property="Background" Value="#FFC6F6C6"/>
									<Setter Property="Background" TargetName="border" Value="{DynamicResource ButtonNormalBackgroundFill}"/>
								</Trigger>
							</ControlTemplate.Triggers>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>

			<Style x:Key="ButtonFocusVisual">
				<Setter Property="Control.Template">
					<Setter.Value>
						<ControlTemplate>
							<Rectangle Margin="3"
								StrokeThickness="1"
								Stroke="Black"
								StrokeDashArray="1 2"
								SnapsToDevicePixels="true"/>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>



			<RadialGradientBrush x:Key="ButtonNormalBackgroundFill"
				>
				<GradientStop Color="#FFE3E5F0" Offset="0.439"/>
				<GradientStop Color="#FFB2E3FF" Offset="1"/>
			</RadialGradientBrush>
			<SolidColorBrush x:Key="ButtonBorder"
				Color="#FF003C74"/>
			<LinearGradientBrush EndPoint="0.25,0.067" StartPoint="0.75,0.933" x:Key="GlassGrad">
				<GradientStop Color="#FF329CA5" Offset="0"/>
				<GradientStop Color="#FFAEF2FA" Offset="1"/>
			</LinearGradientBrush>
			<Style
				TargetType="{x:Type Button}" x:Key="Magnify">
				<Setter Property="FocusVisualStyle"
					Value="{StaticResource ButtonFocusVisual}"/>
				<Setter Property="Background"
					Value="{StaticResource ButtonNormalBackgroundFill}"/>
				<Setter Property="BorderBrush"
					Value="{StaticResource ButtonBorder}"/>
				<Setter Property="Foreground"
					Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
				<Setter Property="HorizontalContentAlignment"
					Value="Center"/>
				<Setter Property="VerticalContentAlignment"
					Value="Center"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="{x:Type ButtonBase}">
							<Border>
								<Canvas>
									<Ellipse Stroke="#FF000000" Width="22" Height="22" Canvas.Left="-0.169" Canvas.Top="-1.336">
										<Ellipse.Fill>
											<LinearGradientBrush EndPoint="0.854,0.854" StartPoint="0.146,0.146">
												<GradientStop Color="#FF7AA2B1" Offset="0"/>
												<GradientStop Color="#FFFFFFFF" Offset="1"/>
											</LinearGradientBrush>
										</Ellipse.Fill>
									</Ellipse>
									<Ellipse Stroke="#FF626262" Width="16" Height="16" Canvas.Left="2.833" Canvas.Top="1.666">
										<Ellipse.Fill>
											<DynamicResource ResourceKey="GlassGrad"/>
										</Ellipse.Fill>
									</Ellipse>
									<Ellipse Stroke="{x:Null}" Width="9.5" Height="9.5" Canvas.Left="5.208" Canvas.Top="3.791">
										<Ellipse.Fill>
											<LinearGradientBrush EndPoint="0.225,0.081" StartPoint="0.864,1.127">
												<GradientStop Color="#BF45B5C1" Offset="0"/>
												<GradientStop Color="#FFFFFFFF" Offset="1"/>
											</LinearGradientBrush>
										</Ellipse.Fill>
									</Ellipse>
									<Ellipse Stroke="{x:Null}" Width="6" Height="6" Canvas.Left="10.375" Canvas.Top="8.625">
										<Ellipse.Fill>
											<LinearGradientBrush EndPoint="0.225,0.081" StartPoint="0.864,1.127">
												<GradientStop Color="#596EBDC5" Offset="0"/>
												<GradientStop Color="#2AFFFFFF" Offset="1"/>
											</LinearGradientBrush>
										</Ellipse.Fill>
									</Ellipse>
									<Rectangle RenderTransformOrigin="0.5,0.5" Stroke="#FF000000" Width="18" Height="6" Canvas.Left="18.389" Canvas.Top="15.757">
										<Rectangle.RenderTransform>
											<TransformGroup>
												<ScaleTransform ScaleX="0.99999999999999989" ScaleY="0.99999999999999989"/>
												<SkewTransform AngleX="0" AngleY="0"/>
												<RotateTransform Angle="28.978000000000005"/>
												<TranslateTransform X="0" Y="0"/>
											</TransformGroup>
										</Rectangle.RenderTransform>
										<Rectangle.Fill>
											<LinearGradientBrush EndPoint="0.488,-0.138" StartPoint="0.498,0.917">
												<GradientStop Color="#FF7AA2B1" Offset="0"/>
												<GradientStop Color="#FFFFFFFF" Offset="1"/>
											</LinearGradientBrush>
										</Rectangle.Fill>
									</Rectangle>
									<Rectangle Stroke="#FF000000" Width="6.375" Height="6" RenderTransformOrigin="0.5,0.5" Canvas.Left="29.452" Canvas.Top="18.828">
										<Rectangle.RenderTransform>
											<TransformGroup>
												<ScaleTransform ScaleX="1" ScaleY="1"/>
												<SkewTransform AngleX="0" AngleY="0"/>
												<RotateTransform Angle="28.82"/>
												<TranslateTransform X="0" Y="0"/>
											</TransformGroup>
										</Rectangle.RenderTransform>
										<Rectangle.Fill>
											<LinearGradientBrush EndPoint="0.497,-0.222" StartPoint="0.497,0.778">
												<GradientStop Color="#FF7AA2B1" Offset="0"/>
												<GradientStop Color="#FFFFFFFF" Offset="1"/>
											</LinearGradientBrush>
										</Rectangle.Fill>
									</Rectangle>
								</Canvas>
							</Border>
							<ControlTemplate.Triggers>
								<Trigger Property="IsKeyboardFocused"
									Value="true"/>
								<Trigger Property="ToggleButton.IsChecked"
									Value="true"/>
								<Trigger Property="IsEnabled"
									Value="false">
									<Setter Property="Foreground"
										Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
								</Trigger>
							</ControlTemplate.Triggers>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>


			<LinearGradientBrush EndPoint="0.501,-0.007" StartPoint="0.501,1.007" x:Key="BlueSteel">
				<GradientStop Color="#FF0B9FF2" Offset="0.787"/>
				<GradientStop Color="#FF007674" Offset="0"/>
				<GradientStop Color="#FF004468" Offset="0.257"/>
				<GradientStop Color="#FF004970" Offset="0.251"/>
				<GradientStop Color="#FF014F79" Offset="0.421"/>
				<GradientStop Color="#FF036194" Offset="1"/>
			</LinearGradientBrush>
			<Style
				TargetType="{x:Type Button}" x:Key="ButtonStyle1">
				<Setter Property="FocusVisualStyle"
					Value="{StaticResource ButtonFocusVisual}"/>
				<Setter Property="Background"
					Value="{StaticResource ButtonNormalBackgroundFill}"/>
				<Setter Property="BorderBrush"
					Value="{StaticResource ButtonBorder}"/>
				<Setter Property="Foreground"
					Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
				<Setter Property="HorizontalContentAlignment"
					Value="Center"/>
				<Setter Property="VerticalContentAlignment"
					Value="Center"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="{x:Type ButtonBase}">
							<Grid>
								<Rectangle Opacity="1" Stroke="{DynamicResource DarkBlue}" RadiusX="4" RadiusY="4" x:Name="rectangle">
									<Rectangle.Fill>
										<DynamicResource ResourceKey="MetalGrad"/>
									</Rectangle.Fill>
								</Rectangle>
								<ContentPresenter HorizontalAlignment="Stretch" x:Name="contentPresenter"/>
							</Grid>
							<ControlTemplate.Triggers>
								<Trigger Property="IsKeyboardFocused"
									Value="true"/>
								<Trigger Property="ToggleButton.IsChecked"
									Value="true"/>
								<Trigger Property="IsEnabled"
									Value="false">
									<Setter Property="Opacity" TargetName="contentPresenter" Value="0.5"/>
									<Setter Property="Opacity" TargetName="rectangle" Value="0.6"/>
								</Trigger>
								<Trigger Property="IsMouseOver" Value="True">
									<Setter Property="Opacity" TargetName="rectangle" Value="1"/>
									<Setter Property="Fill" TargetName="rectangle" Value="{DynamicResource ButtonNormalBackgroundFill}"/>
								</Trigger>
							</ControlTemplate.Triggers>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>


			<Storyboard x:Key="CloseOut">
				<ThicknessAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="grid" Storyboard.TargetProperty="(FrameworkElement.Margin)">
					<SplineThicknessKeyFrame KeyTime="00:00:00.3000000" Value="0,0,-13,0"/>
				</ThicknessAnimationUsingKeyFrames>
			</Storyboard>
			<Storyboard x:Key="CloseIn">
				<ThicknessAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="grid" Storyboard.TargetProperty="(FrameworkElement.Margin)">
					<SplineThicknessKeyFrame KeyTime="00:00:01.0000000" Value="0,0,-13,0"/>
					<SplineThicknessKeyFrame KeyTime="00:00:01.3000000" Value="0,0,4,0"/>
				</ThicknessAnimationUsingKeyFrames>
			</Storyboard>
			<Style x:Key="checkBoxStyle" TargetType="{x:Type CheckBox}">
				<Setter Property="SnapsToDevicePixels" Value="true"/>
				<Setter Property="FocusVisualStyle" Value="{DynamicResource CheckBoxFocusVisual}"/>
				<Setter Property="Background" Value="{DynamicResource NormalBrush}"/>
				<Setter Property="BorderBrush" Value="{DynamicResource NormalBorderBrush}"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="{x:Type CheckBox}">
							<!-- BulletDecorator is used to provide baseline alignment between the checkmark and the Content -->
							<BulletDecorator Background="Transparent">
								<BulletDecorator.Bullet>
									<Grid Width="13" Height="13">
										<Border x:Name="Border" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"/>
										<Path x:Name="CheckMark" Stroke="{DynamicResource GlyphBrush}" StrokeThickness="2" SnapsToDevicePixels="False" Data="M 3 3 L 10 10 M 3 10 L 10 3"/>
									</Grid>
								</BulletDecorator.Bullet>
								<ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" RecognizesAccessKey="True"/>
							</BulletDecorator>
							<!-- This uses Visibility to hide and show the CheckMark on IsChecked -->
							<ControlTemplate.Triggers>
								<Trigger Property="IsChecked" Value="false">
									<Setter Property="Visibility" Value="Collapsed" TargetName="CheckMark"/>
								</Trigger>
								<Trigger Property="IsMouseOver" Value="true">
									<Setter Property="Background" Value="{DynamicResource MouseOverBrush}" TargetName="Border"/>
								</Trigger>
								<Trigger Property="IsPressed" Value="true">
									<Setter Property="Background" Value="{DynamicResource PressedBrush}" TargetName="Border"/>
									<Setter Property="BorderBrush" Value="{DynamicResource PressedBorderBrush}" TargetName="Border"/>
								</Trigger>
								<Trigger Property="IsEnabled" Value="false">
									<Setter Property="Background" Value="{DynamicResource DisabledBackgroundBrush}" TargetName="Border"/>
									<Setter Property="BorderBrush" Value="{DynamicResource DisabledBorderBrush}" TargetName="Border"/>
								</Trigger>
							</ControlTemplate.Triggers>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
		</ResourceDictionary>
	</Window.Resources>

	<Window.Triggers>
		<EventTrigger RoutedEvent="Mouse.MouseEnter">
			<BeginStoryboard Storyboard="{StaticResource CloseOut}"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseLeave">
			<BeginStoryboard x:Name="CloseIn_BeginStoryboard" Storyboard="{StaticResource CloseIn}"/>
		</EventTrigger>
	</Window.Triggers>

	<Grid Name="DocumentRoot" Margin="0,0,13,0" ClipToBounds="False" Width="Auto">
		<Grid.RowDefinitions>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>

		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="150"/>
			<ColumnDefinition Width="*"/>
			<ColumnDefinition Width="Auto"/>
			<ColumnDefinition Width="Auto"/>
			<ColumnDefinition Width="Auto"/>
		</Grid.ColumnDefinitions>
		<Grid HorizontalAlignment="Right" Margin="0,0,4,0" x:Name="grid" Width="18" Height="18" Grid.Column="4" MouseLeftButtonDown="HandleClose" ToolTip="Close">
			<Ellipse Fill="#FF5B0000"/>
			<Path Stretch="Fill" Stroke="#FFFFFFFF" StrokeThickness="3" Margin="4,4,4,4" Data="M18.33996,8.7972167 L1.8622991,25.274878" Canvas.Left="4" Canvas.Top="4"/>
			<Path Fill="#FFFFFFFF" Stretch="Fill" Stroke="#FFFFFFFF" StrokeThickness="3" Margin="4,4,4,4" Data="M19.831014,18.751491 L4.5959764,3.5164535" Canvas.Left="4" Canvas.Top="4"/>
		</Grid>
		<Border d:LayoutOverrides="GridBox" Margin="1,1,0,0" Grid.ColumnSpan="5" Grid.RowSpan="1" Background="#66000000" CornerRadius="3,3,3,3"/>
		<Border
			Grid.ColumnSpan="5"
			Grid.RowSpan="1"
			Background="{DynamicResource BlueSteel}"
			BorderBrush="#66FFFFFF"
			BorderThickness="1,1,1,1"
			CornerRadius="2,2,2,2"
			d:LayoutOverrides="GridBox"
			Margin="0,0,1,1"
		/>

		<StackPanel Orientation="Horizontal">
			<TextBlock
				Text="SNOOP"
				Margin="4,0,0,0"
				VerticalAlignment="Center"
				Width="Auto"
				Height="Auto"
				FontFamily="VDub"
				FontSize="18"
				FontWeight="Normal"
				Foreground="#B2FFFFFF"
				TextAlignment="Center"
				IsEnabled="True"
				Opacity="0.745"
			/>
			<TextBlock
				x:Name="snoopTextBlock"
				Margin="4,0,0,0"
				VerticalAlignment="Center"
				Width="Auto"
				Height="Auto"
				FontFamily="VDub"
				FontSize="10"
				FontWeight="Normal"
				Foreground="#B2FFFFFF"
				TextAlignment="Center"
				IsEnabled="True"
				Opacity="0.745" Text="(32-bit)"
			/>
			<TextBlock
				Margin="4,0,0,0"
				VerticalAlignment="Center"
				Width="Auto"
				Height="Auto"
				FontFamily="VDub"
				FontSize="10"
				FontWeight="Normal"
				Foreground="#B2FFFFFF"
				TextAlignment="Center"
				IsEnabled="True"
				Opacity="0.745"
				Text="(WPF 3.5)"
			/>
		</StackPanel>

		<Button
			Height="20"
			VerticalAlignment="Center"
			ToolTip="Refresh (F5)"
			Padding="0"
			Background="Green"
			Style="{StaticResource ReloadButton}"
			d:LayoutOverrides="GridBox"
			HorizontalAlignment="Center"
			Margin="8,0,0,0"
			Width="20"
			BorderBrush="#FF0E0E0E"
			Foreground="#FF000000"
			Grid.Column="2"
			Command="{x:Static my:AppChooser.RefreshCommand}"
		>
			<Grid>
				<Path Fill="{DynamicResource DeepBlueColorBrush}" Stretch="Fill" Stroke="{x:Null}" Margin="-0.104,3.939,9.354,1.686" Data="M4.333,4 L0.37273436,10.3125 3.0196258,10.3125 C3.0377636,15.483316 5.2010074,17.342296 10.143806,17.401491 L10.143806,14.916038 C6.453455,14.89251 5.5418587,13.089075 5.7292816,10.250495 L8.353837,10.250495 z"/>
				<Path Fill="{DynamicResource DeepBlueColorBrush}" Stretch="Fill" Stroke="{x:Null}" Data="M4.333,4 L0.37273436,10.3125 3.0196258,10.3125 C3.0377636,15.483316 5.2010074,17.342296 10.143806,17.401491 L10.143806,14.916038 C6.453455,14.89251 5.5418587,13.089075 5.7292816,10.250495 L8.353837,10.250495 z" RenderTransformOrigin="0.5,0.5" Margin="9.266,1.564,-0.021,4.061">
					<Path.RenderTransform>
						<TransformGroup>
							<ScaleTransform ScaleX="1" ScaleY="1"/>
							<SkewTransform AngleX="0" AngleY="0"/>
							<RotateTransform Angle="180"/>
							<TranslateTransform X="0" Y="0"/>
						</TransformGroup>
					</Path.RenderTransform>
				</Path>
			</Grid>
		</Button>
		<CheckBox
			IsChecked="{Binding AutoRefresh}"
			DockPanel.Dock="Right"
			VerticalAlignment="Center"
			Margin="4,0,0,0"
			Grid.Column="3"
			ToolTip="Auto Refresh - check this box to automatically (in the background) refresh the list of applications that can be Snoop(ed)."
			Style="{DynamicResource checkBoxStyle}"
		/>

		<ComboBox
			Margin="0,0,0,0"
			MinWidth="160"
			x:Name="AppCombo"
			Style="{DynamicResource SimpleComboBox}"
			VerticalAlignment="Center"
			Height="20"
			BorderThickness="0,0,0,0"
			Grid.Column="1"
			ItemsSource="{Binding Path=Windows}"
			IsSynchronizedWithCurrentItem="True"
			Foreground="#FFFFFFFF"
		/>

		<StackPanel Margin="4,0,8,0" Grid.Column="4" Orientation="Horizontal" Height="20">
			<!--<Button Content="Cancel" IsCancel="True" Width="70" Margin="0,0,2,0"/>-->
			<Button
				Width="40"
				HorizontalAlignment="Stretch"
				x:Name="Button_Mag"
				Style="{DynamicResource ButtonStyle1}"
				VerticalAlignment="Stretch"
				Grid.Column="2"
				ToolTip="Magnify"
				Command="{x:Static my:AppChooser.MagnifyCommand}"
				>
				<Viewbox Height="16">
					<Canvas Width="53.613" Height="26.017" Margin="0,3,0,0">
						<Ellipse Stroke="#FF000000" Width="22" Grid.Column="3" Height="22" Canvas.Left="4.046" Canvas.Top="-2.627">
							<Ellipse.Fill>
								<LinearGradientBrush EndPoint="0.854,0.854" StartPoint="0.146,0.146">
									<GradientStop Color="#FF7AA2B1" Offset="0"/>
									<GradientStop Color="#FFFFFFFF" Offset="1"/>
								</LinearGradientBrush>
							</Ellipse.Fill>
						</Ellipse>
						<Ellipse Stroke="#FF626262" Width="16" Grid.Column="3" Height="16" Canvas.Left="7.048" Canvas.Top="0.375">
							<Ellipse.Fill>
								<DynamicResource ResourceKey="GlassGrad"/>
							</Ellipse.Fill>
						</Ellipse>
						<Ellipse Stroke="{x:Null}" Width="9.5" Grid.Column="3" Height="9.5" Canvas.Left="9.423" Canvas.Top="2.5">
							<Ellipse.Fill>
								<LinearGradientBrush EndPoint="0.225,0.081" StartPoint="0.864,1.127">
									<GradientStop Color="#BF45B5C1" Offset="0"/>
									<GradientStop Color="#FFFFFFFF" Offset="1"/>
								</LinearGradientBrush>
							</Ellipse.Fill>
						</Ellipse>
						<Ellipse Stroke="{x:Null}" Width="6" Grid.Column="3" Height="6" Canvas.Left="14.59" Canvas.Top="7.334">
							<Ellipse.Fill>
								<LinearGradientBrush EndPoint="0.225,0.081" StartPoint="0.864,1.127">
									<GradientStop Color="#596EBDC5" Offset="0"/>
									<GradientStop Color="#2AFFFFFF" Offset="1"/>
								</LinearGradientBrush>
							</Ellipse.Fill>
						</Ellipse>
						<Rectangle RenderTransformOrigin="0.5,0.5" Stroke="#FF000000" Grid.Column="3" Width="18" Height="6" Canvas.Left="22.604" Canvas.Top="14.466">
							<Rectangle.RenderTransform>
								<TransformGroup>
									<ScaleTransform ScaleX="0.99999999999999978" ScaleY="0.99999999999999978"/>
									<SkewTransform AngleX="0" AngleY="0"/>
									<RotateTransform Angle="28.977999999999984"/>
									<TranslateTransform X="0" Y="0"/>
								</TransformGroup>
							</Rectangle.RenderTransform>
							<Rectangle.Fill>
								<LinearGradientBrush EndPoint="0.488,-0.138" StartPoint="0.498,0.917">
									<GradientStop Color="#FF7AA2B1" Offset="0"/>
									<GradientStop Color="#FFFFFFFF" Offset="1"/>
								</LinearGradientBrush>
							</Rectangle.Fill>
						</Rectangle>
						<Rectangle Stroke="#FF000000" RenderTransformOrigin="0.5,0.5" Grid.Column="3" Width="6.375" Height="6" Canvas.Left="33.667" Canvas.Top="17.537">
							<Rectangle.RenderTransform>
								<TransformGroup>
									<ScaleTransform ScaleX="0.99999999999999989" ScaleY="0.99999999999999978"/>
									<SkewTransform AngleX="0" AngleY="0"/>
									<RotateTransform Angle="28.82"/>
									<TranslateTransform X="0" Y="0"/>
								</TransformGroup>
							</Rectangle.RenderTransform>
							<Rectangle.Fill>
								<LinearGradientBrush EndPoint="0.497,-0.222" StartPoint="0.497,0.778">
									<GradientStop Color="#FF7AA2B1" Offset="0"/>
									<GradientStop Color="#FFFFFFFF" Offset="1"/>
								</LinearGradientBrush>
							</Rectangle.Fill>
						</Rectangle>
					</Canvas>
				</Viewbox>
			</Button>

			<Button
				IsDefault="True"
				Width="40"
				Margin="4,0,0,0"
				HorizontalAlignment="Right"
				x:Name="Button_Snoop"
				Style="{DynamicResource ButtonStyle1}"
				VerticalAlignment="Stretch"
				Grid.Column="2"
				ToolTip="Snoop this application"
				Command="{x:Static my:AppChooser.InspectCommand}"
